HOST_UID=1000

# MySQL
MYSQL_DATABASE=wpfilemanagersolid
MYSQL_USER=admin
MYSQL_PASSWORD=admin
MYSQL_ROOT_PASSWORD=123456

# WP & WP Staging
WP_ADMIN_USERNAME=admin
WP_ADMIN_PASSWORD=admin
WP_ADMIN_EMAIL=dev@wp-staging.com
WP_DEBUG_LOG=/dev/stderr
