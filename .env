DOCKER_SUBNET=172.202.0.0/16
DOCKER_GATEWAY_IP=172.202.0.1
DOCKER_NETWORK=wp-file-manager-solid-network
COMPOSE_PROJECT_NAME=wp-file-manager-solid
HOST_NAME=wp-file-manager-solid.local
WAIT_SERVICES_IN_SECONDS=15

# Docker Development Environment Permissions
